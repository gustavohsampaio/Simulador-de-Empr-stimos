his.camposErros = [];\n\n      if (this.nome == \"\") this.camposErros.push(\"nome\");\n\n      if (this.sobrenome == \"\") this.camposErros.push(\"sobrenome\");\n\n      if (this.email == \"\") this.camposErros.push(\"email\");\n\n      if (this.nascimento == \"\") this.camposErros.push(\"nascimento\");\n\n      if (this.endereco == \"\") this.camposErros.push(\"endereco\");\n\n      if (this.SelecionarEstado == null) this.camposErros.push(\"estado\");\n\n      if (this.cidade == \"\") this.camposErros.push(\"cidade\");\n\n      if (this.cep == \"\") this.camposErros.push(\"cep\");\n\n      if (this.dev == \"\") this.camposErros.push(\"dev\");\n\n      if (this.SelecionarSenioridade == null)\n        this.camposErros.push(\"senioridade\");\n\n      if (this.linguagens.length == 0) this.camposErros.push(\"linguagens\");\n\n      if (this.experiencia == \"\") this.camposErros.push(\"experiencia\");\n\n      if (this.camposErros.length > 0)\n        this.$toast.add({\n          severity: \"error\",\n          summary: \"Informações Inválidas\",\n          detail: \"Preencha corretamente todas as informações!\",\n          life: 3000,\n        });\n      else\n        this.$toast.add({\n          severity: \"success\",\n          summary: \"OK\",\n          detail: \"Informações corretas!\",\n          life: 3000,\n        });\n    },\n  },\n};\n</script>\n\n<style>\n</style>\n"],"sourceRoot":""}]}